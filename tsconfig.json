{
	"compilerOptions": {
		"target": "ES6",
		"module": "es2015",
		"moduleResolution": "node",
		"outDir": "./dist/",
		"esModuleInterop": true,
		"declaration": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"baseUrl": "./src/",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
